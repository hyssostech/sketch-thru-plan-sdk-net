{
  "App": {
    "StpConnection": "localhost:9555",
    "MapImagePath": "Resources\\bogoland.png",
    "MapTopLat": "58.78432",
    "MapLeftLon": "15.51776",
    "MapBottomLat": "58.36185",
    "MapRightLon": "16.96658"
  },
    "Logging": {
        "LogLevel": {
            "Default": "Trace",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Warning"
        },
        "Debug": {
            "LogLevel": {
                "Default": "Debug"
            }
        },
        "Console": {
            "IncludeScopes": true,
            "LogLevel": {
                "Default": "Trace"
            }
        }
    },
    "NLog": {
        "internalLogLevel": "Debug",
        "internalLogFile": "c:\\temp\\internal-nlog.txt",
        "throwConfigExceptions": true,
        "extensions": [
            {
                "assembly": "NLog.Extensions.Logging"
            }
        ],
        "targets": {
            "async": true,
            "logfile": {
                "type": "File",
                "fileName": "${specialfolder:folder=CommonApplicationData:cached=true}/STP/Logs/${processname}-${shortdate}.log",
                "layout": "${longdate} | ${level} | ${callsite} | ${message} | ${exception:format=ToString}"
            },
            "logconsole": {
                "type": "Console",
                "layout": "[${level}] ${message} > ${exception:format=ToString}"
            }
        },
        "rules": [
            {
                "logger": "*",
                "minlevel": "Warning",
                "writeTo": "logconsole"
            },
            {
                "logger": "*",
                "minlevel": "Trace",
                "writeTo": "logfile"
            }
        ]
    }
}